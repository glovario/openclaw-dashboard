{
  "name": "openclaw-dashboard",
  "version": "1.0.0",
  "description": "Collaborative task dashboard for the OpenClaw agent team",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev:server": "node --watch src/server.js",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "seed": "node src/seed.js",
    "test:reports-contract": "node scripts/test-reports-contract.js",
    "test:reports-reconciliation": "node scripts/test-reports-reconciliation.js",
    "generate:reports-fixtures": "node scripts/generate-reports-fixtures.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^8.2.1",
    "sql.js": "^1.11.0"
  }
}
